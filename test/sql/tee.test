# name: test/sql/quack.test
# description: test quack extension
# group: [sql]

# Before we load the extension, this will fail
statement error
SELECT * FROM tee((SELECT * FROM (VALUES (1,2)))) _(a,b) WHERE a < 1;
----
Catalog Error: Table Function with name tee does not exist!

# Require statement will ensure this test is run with this extension loaded
require tee

# Confirm the extension works
query II
SELECT * FROM tee((SELECT * FROM (VALUES (1,2)))) _(a,b) WHERE a < 1;
----

# Only 1 pair of parentheses should work
query II
SELECT * FROM tee(SELECT * FROM (VALUES (1,2))) _(a,b) WHERE a < 1;
----

# Should work
query I
SELECT * FROM tee(SELECT * FROM range(3)) _(x);
----
0
1
2

# SHould work
query I
SELECT x FROM tee((SELECT * FROM range(10))) _(x) WHERE x >= 8;
----
8
9

# Should work
query I
SELECT x*2 FROM tee((SELECT * FROM range(3))) _(x);
----
0
2
4

# Should work
query I
SELECT sum(x) FROM tee((SELECT * FROM range(5))) _(x);
----
10


# Should work
query I
SELECT * FROM tee((SELECT * FROM range(0))) _(x);
----


